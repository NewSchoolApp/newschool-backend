{
  "name": "back",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "prestart:prod": "rimraf dist && npm run build",
    "postinstall": "npm run prestart:prod",
    "lint": "eslint \"src/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.config.js --runInBand",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/ormconfig.ts",
    "typeorm:create-migration": "npm run typeorm migration:create -- -n",
    "typeorm:generate-migration": "npm run typeorm migration:generate -- -n",
    "typeorm:run-migration": "npm run typeorm migration:run",
    "typeorm:revert-migration": "npm run typeorm migration:revert"
  },
  "dependencies": {
    "@nest-modules/mailer": "1.3.22",
    "@nestjs/common": "7.6.14",
    "@nestjs/config": "0.6.3",
    "@nestjs/core": "7.6.14",
    "@nestjs/jwt": "7.2.0",
    "@nestjs/platform-express": "7.6.14",
    "@nestjs/schedule": "0.4.3",
    "@nestjs/swagger": "4.7.16",
    "@nestjs/typeorm": "7.1.5",
    "@sentry/node": "6.2.2",
    "@sentry/tracing": "6.2.2",
    "aws-sdk": "2.856.0",
    "cache-manager": "3.4.1",
    "class-transformer": "0.3.1",
    "class-validator": "0.13.1",
    "dotenv": "8.2.0",
    "dotenv-flow": "3.2.0",
    "export-from-json": "1.3.4",
    "mysql": "2.18.1",
    "nest-raven": "7.2.0",
    "nodemailer": "6.5.0",
    "passport-facebook-token": "4.0.0",
    "passport-jwt": "4.0.0",
    "pubsub-js": "1.9.3",
    "pusher": "5.0.0",
    "reflect-metadata": "0.1.13",
    "remove": "0.1.5",
    "rimraf": "3.0.2",
    "rxjs": "6.6.6",
    "slugify": "1.4.7",
    "swagger-ui-express": "4.1.6",
    "typeorm": "0.2.31",
    "typeorm-transactional-cls-hooked": "0.1.20",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "7.5.6",
    "@nestjs/schematics": "7.2.8",
    "@nestjs/testing": "7.6.14",
    "@types/dotenv": "8.2.0",
    "@types/express": "4.17.11",
    "@types/jest": "26.0.21",
    "@types/multer": "1.4.5",
    "@types/mysql": "2.15.18",
    "@types/node": "13.13.45",
    "@types/nodemailer": "6.4.1",
    "@types/passport-jwt": "3.0.5",
    "@types/pubsub-js": "1.8.2",
    "@types/supertest": "2.0.10",
    "@typescript-eslint/eslint-plugin": "4.18.0",
    "@typescript-eslint/parser": "4.18.0",
    "axios": "0.21.1",
    "eslint": "7.22.0",
    "eslint-config-prettier": "7.2.0",
    "husky": "5.1.3",
    "jest": "26.6.3",
    "prettier": "2.2.1",
    "supertest": "6.1.3",
    "ts-jest": "26.5.4",
    "ts-loader": "8.0.17",
    "ts-node": "9.1.1",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.2.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format"
    }
  }
}
