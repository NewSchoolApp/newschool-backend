{
  "name": "back",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "prestart:prod": "rimraf dist && npm run build",
    "postinstall": "npm run prestart:prod",
    "lint": "eslint \"src/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.config.js --runInBand"
  },
  "dependencies": {
    "@nest-modules/mailer": "^1.1.3",
    "@nestjs/common": "^6.11.1",
    "@nestjs/config": "0.1.0",
    "@nestjs/core": "^6.11.1",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/platform-express": "^6.11.1",
    "@nestjs/swagger": "^4.2.3",
    "@nestjs/typeorm": "^6.2.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "dotenv-flow": "^3.1.0",
    "i": "^0.3.6",
    "mysql": "^2.18.1",
    "npm": "^6.13.6",
    "passport-facebook-token": "^3.3.0",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "remove": "^0.1.5",
    "rimraf": "^3.0.0",
    "rollbar": "^2.14.4",
    "rxjs": "^6.5.4",
    "slugify": "^1.3.6",
    "swagger-ui-express": "^4.1.3",
    "typeorm": "^0.2.22",
    "typeorm-transactional-cls-hooked": "^0.1.10"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.14.1",
    "@nestjs/schematics": "^6.8.2",
    "@nestjs/testing": "^6.11.1",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/jest": "^24.9.1",
    "@types/node": "^13.5.0",
    "@types/passport-jwt": "^3.0.3",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "husky": "^4.2.1",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "^25.0.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format"
    }
  }
}
